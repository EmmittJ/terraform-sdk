{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "myapp": {
      "type": "container.v0",
      "image": "image:latest"
    },
    "myapp-infrastructure": {
      "error": "This resource does not support generation in the manifest."
    },
    "myapp-infrastructure-region": {
      "type": "parameter.v0",
      "value": "{myapp-infrastructure-region.inputs.value}",
      "inputs": {
        "value": {
          "type": "string"
        }
      }
    }
  }
}
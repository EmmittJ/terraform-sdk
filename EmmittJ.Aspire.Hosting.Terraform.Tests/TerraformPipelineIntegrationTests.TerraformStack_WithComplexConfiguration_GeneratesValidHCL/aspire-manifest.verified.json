{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "myapp": {
      "type": "container.v0",
      "image": "image:latest"
    },
    "myapp-complex": {
      "error": "This resource does not support generation in the manifest."
    },
    "myapp-complex-environment": {
      "type": "parameter.v0",
      "value": "{myapp-complex-environment.inputs.value}",
      "inputs": {
        "value": {
          "type": "string"
        }
      }
    },
    "myapp-complex-instance-count": {
      "type": "parameter.v0",
      "value": "{myapp-complex-instance-count.inputs.value}",
      "inputs": {
        "value": {
          "type": "string"
        }
      }
    },
    "myapp-complex-enable-monitoring": {
      "type": "parameter.v0",
      "value": "{myapp-complex-enable-monitoring.inputs.value}",
      "inputs": {
        "value": {
          "type": "string"
        }
      }
    }
  }
}